{
  "name": "rudnev/laravel-settings",
  "description": "Persistent settings for Laravel Framework",
  "keywords": [
    "rudnev",
    "laravel",
    "settings"
  ],
  "homepage": "https://github.com/rudnev/laravel-settings",
  "license": "MIT",
  "authors": [
    {
      "name": "Andrei Rudnev",
      "email": "andrey@rudnev.org",
      "homepage": "https://rudnev.org",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.1|^8.0",
    "ext-json": "*",
    "illuminate/contracts": "^5.5|^6.0|^7.0|^8.0|^9.0",
    "illuminate/database": "^5.5|^6.0|^7.0|^8.0|^9.0",
    "illuminate/events": "^5.5|^6.0|^7.0|^8.0|^9.0",
    "illuminate/support": "^5.5|^6.0|^7.0|^8.0|^9.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.3",
    "orchestra/testbench": "^3.6|^4.0|^5.0|^6.0|^7.0",
    "phpunit/phpunit": "^7.0|^8.0|^9.0"
  },
  "autoload": {
    "files": [
      "src/helpers.php"
    ],
    "psr-4": {
      "Rudnev\\Settings\\": "src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Rudnev\\Settings\\ServiceProvider"
      ],
      "aliases": {
        "Settings": "Rudnev\\Settings\\Facades\\SettingsFacade"
      }
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Rudnev\\Settings\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit --colors=always"
  },
  "config": {
    "sort-packages": true
  }
}
